generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model quiz_table {
  id        Int     @id @default(autoincrement())
  question  String
  answer_id String  @db.Uuid @default(dbgenerated("gen_random_uuid()"))
  answer    String
}
